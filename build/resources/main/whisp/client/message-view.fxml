<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="messageHbox" alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="whisp.client.MessageViewController">
   <children>
      <HBox alignment="CENTER" style="-fx-border-radius: 1; -fx-border-width: 2; -fx-border-color: #000000;">
         <children>
            <Label fx:id="messageLabel" text="Text" wrapText="true">
               <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets right="100.0" />
   </padding>
</HBox>
